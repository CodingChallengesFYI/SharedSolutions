{
  "resources": {
    "includes": [
      {
        "pattern": "\\QMETA-INF/services/org/apache/activemq/transport/tcp\\E",
        "condition": {
          "typeReachable": "org.apache.activemq.transport.TransportFactory"
        }
      },
      {
        "pattern": "\\QMETA-INF/services/org/apache/activemq/wireformat/default\\E",
        "condition": {
          "typeReachable": "org.apache.activemq.transport.tcp.TcpTransportFactory"
        }
      },
      {
        "pattern": "\\Qorg/apache/activemq/version.txt\\E",
        "condition": {
          "typeReachable": "org.apache.activemq.broker.BrokerService"
        }
      }
    ]
  },
  "bundles": [

  ]
}
